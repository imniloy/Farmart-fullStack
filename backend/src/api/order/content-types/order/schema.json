    },
    "method": {
      "type": "string",
      "default": "cash",
      "required": true
    },
    "shipping_address": {
      "type": "json",
      "required": true
    },
    "shipping_method_and_cost": {
      "type": "json",
      "required": true
    },
    "user_personal_details": {
      "type": "json",
      "required": true
    },
    "userId": {
      "type": "string",
      "required": true
    }
  }
